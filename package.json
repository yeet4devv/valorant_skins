{
  "devDependencies": {
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1"
  },
  "scripts": {
  "lint": "eslint \"js/**/*.js\"",
  "lint:fix": "eslint \"js/**/*.js\" --fix",
  "format": "prettier --write \"**/*.{html,css,js,md}\"",
  "format:check": "prettier --check \"**/*.{html,css,js,md}\""
}
}

